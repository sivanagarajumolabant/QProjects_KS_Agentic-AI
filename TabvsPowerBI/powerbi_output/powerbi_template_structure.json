      {
        "name": "SalesAmount",
        "dataType": "Double",
        "sourceColumn": "SalesAmount",
        "summarizeBy": "Sum"
      },
      {
        "name": "Quantity",
        "dataType": "Int64",
        "sourceColumn": "Quantity",
        "summarizeBy": "Sum"
      },
      {
        "name": "Profit",
        "dataType": "Double",
        "sourceColumn": "Profit",
        "summarizeBy": "Sum"
      },
      {
        "name": "Sales Category",
        "dataType": "String",
        "expression": "IF(SUM(Sales[SalesAmount]) > 10000, \"High Sales\", IF(SUM(Sales[SalesAmount]) > 5000, \"Medium Sales\", \"Low Sales\"))",
        "isCalculated": true
      },
      {
        "name": "Year",
        "dataType": "Int64",
        "expression": "YEAR(Sales[OrderDate])",
        "isCalculated": true
      },
      {
        "name": "Quarter",
        "dataType": "String",
        "expression": "\"Q\" & FORMAT(Sales[OrderDate], \"Q\")",
        "isCalculated": true
      }
    ],
    "measures": [
      {
        "name": "Profit Margin",
        "expression": "DIVIDE(SUM(Sales[Profit]), SUM(Sales[SalesAmount]), 0)",
        "formatString": "0.00%;-0.00%;0.00%",
        "description": "Profit margin percentage calculation"
      },
      {
        "name": "Total Sales",
        "expression": "SUM(Sales[SalesAmount])",
        "formatString": "\"$\"#,0.00;(\"$\"#,0.00)",
        "description": "Total sales amount"
      },
      {
        "name": "Total Profit",
        "expression": "SUM(Sales[Profit])",
        "formatString": "\"$\"#,0.00;(\"$\"#,0.00)",
        "description": "Total profit amount"
      },
      {
        "name": "Total Quantity",
        "expression": "SUM(Sales[Quantity])",
        "formatString": "#,0",
        "description": "Total quantity sold"
      },
      {
        "name": "Average Order Value",
        "expression": "DIVIDE(SUM(Sales[SalesAmount]), DISTINCTCOUNT(Sales[OrderID]), 0)",
        "formatString": "\"$\"#,0.00;(\"$\"#,0.00)",
        "description": "Average value per order"
      },
      {
        "name": "Order Count",
        "expression": "DISTINCTCOUNT(Sales[OrderID])",
        "formatString": "#,0",
        "description": "Total number of orders"
      },
      {
        "name": "Customer Count",
        "expression": "DISTINCTCOUNT(Sales[CustomerID])",
        "formatString": "#,0",
        "description": "Total number of customers"
      }
    ],
    "hierarchies": [
