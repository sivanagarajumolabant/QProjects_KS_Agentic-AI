            <column-instance column='[SalesAmount]' derivation='Sum' name='[sum:SalesAmount:qk]' pivot='key' type='quantitative' />
          </datasource-dependencies>
          <aggregation value='true' />
          <shelf-sorts>
            <shelf-sort-rule field='[federated.0abc123d].[sum:SalesAmount:qk]' is-ascending='false' />
          </shelf-sorts>
          <shelves>
            <shelf name='rows'>
              <field>[federated.0abc123d].[none:Region:nk]</field>
            </shelf>
            <shelf name='columns'>
              <field>[federated.0abc123d].[sum:SalesAmount:qk]</field>
            </shelf>
            <shelf name='color'>
              <field>[federated.0abc123d].[none:Region:nk]</field>
            </shelf>
          </shelves>
          <style>
            <style-rule element='mark'>
              <encoding attr='color' field='[federated.0abc123d].[none:Region:nk]' type='palette'>
                <map to='#1f77b4'>
                  <bucket>&quot;East&quot;</bucket>
