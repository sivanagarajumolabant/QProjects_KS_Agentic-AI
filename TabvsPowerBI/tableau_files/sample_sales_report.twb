      <!-- Calculated Fields -->
      <column caption='Profit Margin' datatype='real' name='[Calculation_001]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([Profit]) / SUM([SalesAmount])' />
      </column>
      <column caption='Sales Category' datatype='string' name='[Calculation_002]' role='dimension' type='nominal'>
        <calculation class='tableau' formula='IF SUM([SalesAmount]) > 10000 THEN "High Sales" ELSEIF SUM([SalesAmount]) > 5000 THEN "Medium Sales" ELSE "Low Sales" END' />
      </column>
      <column caption='Year' datatype='integer' name='[Calculation_003]' role='dimension' type='ordinal'>
        <calculation class='tableau' formula='YEAR([OrderDate])' />
      </column>
      <column caption='Quarter' datatype='string' name='[Calculation_004]' role='dimension' type='ordinal'>
        <calculation class='tableau' formula='"Q" + STR(DATEPART("quarter", [OrderDate]))' />
      </column>
      
      <!-- Parameters -->
      <column caption='Region Filter' datatype='string' name='[Parameter 1]' param-domain-type='list' role='measure' type='nominal' value='&quot;All&quot;'>
        <calculation class='tableau' formula='&quot;All&quot;' />
        <members>
          <member alias='All' value='&quot;All&quot;' />
          <member alias='North' value='&quot;North&quot;' />
          <member alias='South' value='&quot;South&quot;' />
          <member alias='East' value='&quot;East&quot;' />
          <member alias='West' value='&quot;West&quot;' />
        </members>
      </column>
      
